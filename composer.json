    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles",
            "php composer.phar self-update"
        ],
        "dev-db-refresh": [
          "dev-db-sync.py DATABASE-IDENTIFIER-HERE"
        ],
        "dev-build": [
          "mkdir -p private && chown $USER:domain\\ users private && chmod 777 private",
          "mkdir -p tmp && chown $USER:domain\\ users tmp && chmod 777 tmp",
          "mkdir -p tmp/php && chown $USER:domain\\ users tmp/php && chmod 777 tmp/php",
          "mkdir -p web/sites/default/files && chown $USER:domain\\ users web/sites/default/files && chmod 777 web/sites/default/files",
          "php composer.phar install -v"
        ],
        "dev-deploy": [
          "cd web && ../vendor/bin/drupal site:maintenance on",
          "cd web && ../vendor/bin/drupal update:execute",
          "cd web && ../vendor/bin/drupal config:import",
          "cd web && ../vendor/bin/drupal config:import",
          "cd web && ../vendor/bin/drupal update:execute",
          "cd web && ../vendor/bin/drupal update:entities",
          "cd web && ../vendor/bin/drupal cron:release",
          "cd web && ../vendor/bin/drupal cron:execute",
          "cd web && ../vendor/bin/drupal site:maintenance off"
        ],
        "dev": [
          "@dev-db-refresh",
          "@dev-build",
          "@dev-deploy"
        ]
    },
    "extra": {
        "installer-types": [
          "mdp-module",
          "mdp-theme"
        ],
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/modules/meredith/{$name}": ["type:mdp-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "web/themes/meredith/{$name}": ["type:mdp-theme"],
            "drush/contrib/{$name}": ["type:drupal-drush"]
        }
    }
